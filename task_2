// По двум заданным числам проверять является ли одно квадратом другого.
Console.WriteLine("Enter number 1: ");
int number1 = int.Parse(Console.ReadLine() ??"0");
Console.WriteLine("Enter number 2: ");
int number2 = int.Parse(Console.ReadLine() ??"0");

 if (number1 == number2 * number2)
 {
   Console.WriteLine("first is a square of second");
 }
 else if (number2 == number1 * number1)
 {
    Console.WriteLine("second is a square of first");
 }

 else   
 {
   Console.WriteLine("first is neither a square of second nor second is square of first");
 }
      